    "        MAX_ORD -= 1\n",
    "        break\n",
    "\n",
    "MAX_CHR = chr(MAX_ORD)\n",
    "MAX_CODE = MAX_CHR.encode(\"unicode-escape\")[2:]\n",
    "print(MAX_ORD == int(MAX_CODE, 16), MAX_ORD)"
